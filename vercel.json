{
    "version": 2,
    "routes": [
      {
        "src": "/(.*).js",
        "headers": {
          "Content-Type": "text/javascript",
          "Access-Control-Allow-Origin": "*"
        },
        "continue": true
      },
      {
        "src": "/",
        "dest": "/index.html"
      },
      {
        "handle": "filesystem"
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          },
          {
            "key": "Cross-Origin-Opener-Policy",
            "value": "same-origin"
          },
          {
            "key": "Cross-Origin-Embedder-Policy",
            "value": "require-corp"
          }
        ]
      },
      {
        "source": "/(.*).js",
        "headers": [
          {
            "key": "Content-Type",
            "value": "text/javascript"
          }
        ]
      }
    ]
  }